    "#interval [`start`, `stop`].\n",
    "\n",
    "#The endpoint of the interval can optionally be excluded.\n",
    "\n",
    "plt.plot(x, x, label='linear')\n",
    "plt.plot(x, x**2, label='quadratic')\n",
    "plt.plot(x, x**3, label='cubic')\n",
    "\n",
